I"(function() {
  var order_status;

  jQuery(function() {
    Stripe.setPublishableKey($('meta[name="stripe-key"]').attr('content'));
    return order_status.setupForm();
  });

  order_status = {
    setupForm: function() {
      return $('#new_order_status').submit(function() {
        $('input[type=submit]').attr('disabled', true);
        if ($('#card_number').length) {
          order_status.processCard();
          return false;
        } else {
          return true;
        }
      });
    },
    processCard: function() {
      var card;
      card = {
        number: $('#card_number').val(),
        cvc: $('#card_code').val(),
        expMonth: $('#card_month').val(),
        expYear: $('#card_year').val()
      };
      return Stripe.createToken(card, order_status.handleStripeResponse);
    },
    handleStripeResponse: function(status, response) {
      if (status === 200) {
        $('#order_status_stripe_card_token').val(response.id);
        return $('#new_order_status')[0].submit();
      } else {
        $('#stripe_error').text(response.error.message);
        return $('input[type=submit]').attr('disabled', false);
      }
    }
  };

}).call(this);
